end {
    Domain string `bor:const(common)`
    Key string `bor:const(Measurement.Begin)`
    Id int `bor:incrByN(10000,1)`
    Value struct {
    	PlanName string `bor:toStrf(incrByN(1,1),A%01s)`
    	BodyNum string `bor:rand(1000000,9999999)`
    	Date string `bor:time(2006/01/02)`
    	Time string `bor:time(15:04:05)`
    	Operator string `bor:const(admin)`
	}
}